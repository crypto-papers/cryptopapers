[ignore]

[include]

[libs]
flow-typed/

[lints]

[options]
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss

module.name_mapper='^_components\/\(.*\)$' -> '<PROJECT_ROOT>/components/\1'
module.name_mapper='^_lib\/\(.*\)$' -> '<PROJECT_ROOT>/lib/\1'
module.name_mapper='^_pages\/\(.*\)$' -> '<PROJECT_ROOT>/pages/\1'
module.name_mapper='^_schema\/\(.*\)$' -> '<PROJECT_ROOT>/schema/\1'
module.name_mapper='^_server\/\(.*\)$' -> '<PROJECT_ROOT>/server/\1'
module.name_mapper='^_static\/\(.*\)$' -> '<PROJECT_ROOT>/static/\1'
module.name_mapper='^_storybook\/\(.*\)$' -> '<PROJECT_ROOT>/.storybook/\1'
module.name_mapper='^styles\/\(.*\)$' -> '<PROJECT_ROOT>/styles/\1'
module.name_mapper='^_types\/\(.*\)$' -> '<PROJECT_ROOT>/types/\1'
module.name_mapper='^_utils\/\(.*\)$' -> '<PROJECT_ROOT>/utils/\1'

[strict]
