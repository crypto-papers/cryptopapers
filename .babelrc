{
  "presets": ["next/babel"],
  "plugins": [
    "babel-plugin-inline-react-svg",
    [
      "babel-plugin-module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "pages": "./pages",
          "static": "./static",
          "styles": "./styles"
        }
      }
    ],
    "babel-plugin-relay",
    "babel-plugin-transform-flow-strip-types",
    [
      "babel-plugin-react-css-modules",
      {
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss"
          }
        },
        "generateScopedName": "[local]_[hash:base64:5]"
      }
    ]
  ]
}
