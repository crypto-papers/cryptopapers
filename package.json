{
  "name": "cryptopapers",
  "version": "1.0.0",
  "description": "Frontend for principal CryptoPapers website",
  "main": "index.js",
  "scripts": {
    "analyze": "ANALYZE=true next build && cp ./.next/analyze/* ./docs/analysis/",
    "build": "next build",
    "dev": "node ./server/server.js",
    "export": "npm run build && next export",
    "flow": "flow",
    "jsdocs": "documentation build ./components/** ./pages/** ./server/** ./.storybook/** ./utils/** -f html -o docs/jsdocs",
    "lint": "eslint --ext .js,.jsx ./components/** ./pages/** ./server/**",
    "pre-flight": "npm run lint && npm run flow",
    "relay": "relay-compiler --src ./ --exclude '**/.next/**' '**/docs/**' '**/__generated__/**' '**/node_modules/**' '**/schema/**' '**/server/**' --schema ./schema/schema.graphql --extensions js jsx",
    "start": "next start -p 8000",
    "storybook": "start-storybook -s ./.storybook/assets"
  },
  "author": "Marek Rewers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crypto-papers/cryptopapers"
  },
  "workspaces": [
    "api"
  ],
  "dependencies": {
    "@babel/core": "^7.12.16",
    "dotenv-webpack": "^6.0.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^10.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-relay": "^10.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@cryptopapers/commitlint-config": "^1.0.0",
    "@cryptopapers/eslint-config": "^2.0.0",
    "@cryptopapers/prettier-config": "^1.1.0",
    "@mdx-js/mdx": "^1.6.22",
    "@next/bundle-analyzer": "^10.0.6",
    "@storybook/react": "^6.1.17",
    "@zeit/next-mdx": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-relay": "^10.1.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "documentation": "^13.1.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "flow-bin": "^0.144.0",
    "graphql": "^15.5.0",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "next-compose": "0.0.2",
    "node-sass": "^5.0.0",
    "postcss-css-variables": "^0.17.0",
    "postcss-nested": "^5.0.3",
    "postcss-scss": "^3.0.4",
    "prettier": "^2.2.1",
    "relay-compiler": "^10.1.3",
    "typescript": "^4.1.5"
  },
  "prettier": "@cryptopapers/prettier-config"
}
