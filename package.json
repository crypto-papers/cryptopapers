{
  "name": "cryptopapers",
  "version": "1.0.0",
  "description": "Frontend for principal CryptoPapers website",
  "main": "index.js",
  "scripts": {
    "analyze": "ANALYZE=true next build && cp ./.next/analyze/* ./docs/analysis/",
    "build": "next build",
    "dev": "node ./server/server.js",
    "export": "npm run build && next export",
    "flow": "flow",
    "jsdocs": "documentation build ./components/** ./pages/** ./server/** ./.storybook/** ./utils/** -f html -o docs/jsdocs",
    "lint": "eslint --ext .js,.jsx ./components/** ./pages/** ./server/**",
    "pre-flight": "npm run lint && npm run flow",
    "relay": "relay-compiler --src ./ --exclude '**/.next/**' '**/docs/**' '**/__generated__/**' '**/node_modules/**' '**/schema/**' '**/server/**' --schema ./schema/schema.graphql --extensions js jsx",
    "start": "next start -p 8000",
    "storybook": "start-storybook -s ./.storybook/assets"
  },
  "author": "Marek Rewers",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crypto-papers/cryptopapers"
  },
  "dependencies": {
    "dotenv-webpack": "^1.8.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-relay": "^9.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@mdx-js/mdx": "^1.6.5",
    "@next/bundle-analyzer": "^9.4.4",
    "@storybook/react": "^5.3.19",
    "@zeit/next-mdx": "^1.2.0",
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-relay": "^9.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "documentation": "^13.0.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "flow-bin": "^0.126.1",
    "graphql": "^15.1.0",
    "jsdoc": "^3.6.4",
    "next-compose": "0.0.2",
    "node-sass": "^4.14.1",
    "postcss-css-variables": "^0.17.0",
    "postcss-nested": "^4.2.1",
    "postcss-scss": "^2.1.1",
    "prettier": "2.0.5",
    "relay-compiler": "^9.1.0"
  }
}
