{
  "include": [
    "src",
    "types"
  ],
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es5",
      "es6"
    ],
    "outDir": "./dist",
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true,
    "rootDir": "./src",
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  }
}